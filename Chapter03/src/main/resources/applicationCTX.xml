<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN/EN"
"http://www.springframework.org/dtd/spring-beans-2.0.dtd">
 <beans>
 	<bean id="student2" class="com.spring.ex01.Student">
 		<property name="name">
 			<value>학생1</value>
 		</property>
 		<property name="age">
 			<value>15</value>
 		</property>
 		<property name="classNumber">
 			<value>A</value>
 		
 		</property>
 		<property name="gradeNumber">
 			<value>4</value>
 		
 		</property>
 	
 	</bean> 
 	<bean id="student3" class="com.spring.ex01.Student">
 		<constructor-arg value="학생3"/>
 		<constructor-arg value="9"/>
 		<constructor-arg value="D"/>
 		<constructor-arg value="2"/>
 	
 	</bean>
 
	<bean id="student1" class="com.spring.ex01.Student">
 		<property name="name">
 			<value>학생2</value>
 		</property>
 		<property name="age">
 			<value>16</value> 
 		</property>
 		<property name="classNumber">
 			<value>C</value>
 		
 		</property>
 		<property name="gradeNumber">
 			<value>5</value>
 		
 		</property>
 	
 	</bean> 	
 	<bean id="studentInfo" class="com.spring.ex01.StudentInfo">
 		<constructor-arg>
 			<ref bean="student1"/>
 		</constructor-arg>
 	</bean>	
 		
 		
 	
 </beans>