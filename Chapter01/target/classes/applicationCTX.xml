<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN/EN"
"http://www.springframework.org/dtd/spring-beans-2.0.dtd">
 
 <beans>	
 <bean id ="calculator" class="com.spring.ex01.Calculator"/>
 <bean id ="myCalculator" class = "com.spring.ex01.MyCalculator">
 	<property name="mycalculator">
 		<ref bean="calculator"/>
 	</property>
 	
 	<property name="firstNum" value="15"></property>
 	<property name="secondNum" value="3"></property>
 
 
 </bean>
 </beans> 