<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.2.xsd"> 
	
	
	<context:annotation-config/>
	
	<bean class="com.spring.ex01.ApplicationConfig"/>
	
	<bean id="student2" class="com.spring.ex01.Student">
		<constructor-arg value="Student2"/>
		<constructor-arg value="17"/>
		<constructor-arg>
			<list>
				<value>Game</value>
				<value>Programming</value>
			</list>
		</constructor-arg>	
		<property name="weight" value="55"/>
		<property name="height" value="177"/>	
	</bean>
	
	
</beans>
